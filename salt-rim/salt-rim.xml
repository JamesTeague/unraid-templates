<?xml version="1.0"?>
<Container version="2">
  <Name>salt-rim</Name>
  <Repository>kmikus12/salt-rim</Repository>
  <Registry>https://hub.docker.com/r/kmikus12/salt-rim/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/kmikus12/salt-rim/</Support>
  <Project>https://github.com/karlomikus/vue-salt-rim</Project>
  <Overview>Salt Rim is a Vue based web client used for connecting to Bar Assistant server instance.&#xD;
&#xD;
https://hub.docker.com/r/kmikus12/salt-rim/</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <Config Name="Container Port 1" Target="8080" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8080</Config>
  <Config Name="Community Applications Conversion" Target="Community_Applications_Conversion" Default="true" Mode="{3}" Description="" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="API_URL" Target="API_URL" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"/>
</Container>
